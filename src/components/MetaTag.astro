---
import Seo from './Seo.astro'
interface Props {
  title: string
  description: string
  image: string
  data: string
  robots: string
}

const { title, description, image, data, robots } = Astro.props as Props
---

<head>
  <title>{title}</title>
  <meta charset="utf-8" />
  <link rel="icon" type="image/svg+xml" href="/favicon.ico" />
  <link rel="sitemap" href="/sitemap-index.xml" />
  <meta name="viewport" content="width=device-width" />
  <meta name="generator" content={Astro.generator} />

  <!-- <Seo {title} {description} url={Astro.url} {image} {data} {robots} /> -->
  <Seo {title} {description} {image} {data} {robots} />

  <!-- Google tag (gtag.js) -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-YPCTPRG03M"
  ></script>
  <script is:inline>
    window.dataLayer = window.dataLayer || []
    function gtag() {
      dataLayer.push(arguments)
    }
    gtag('js', new Date())

    gtag('config', 'G-YPCTPRG03M')
  </script>
</head>
